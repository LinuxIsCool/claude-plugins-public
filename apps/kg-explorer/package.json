{
  "name": "kg-explorer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:mock": "npx tsx scripts/dev-server.ts --mock",
    "dev:full": "npx tsx scripts/dev-server.ts --mock --verbose",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src/__tests__",
    "test:integration": "vitest run tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:perf": "vitest run tests/performance/benchmarks.ts",
    "test:regression": "vitest run tests/regression",
    "test:all": "npm run test && npm run test:e2e",
    "codegen": "npm run codegen:types && npm run codegen:graphql && npm run codegen:fixtures",
    "codegen:types": "npx tsx codegen/generate-types.ts",
    "codegen:graphql": "npx tsx codegen/generate-graphql.ts",
    "codegen:fixtures": "npx tsx codegen/generate-fixtures.ts",
    "playground": "npx tsx scripts/playground-server.ts",
    "kg:query": "npx tsx cli/kg-query.ts",
    "kg:import": "npx tsx cli/kg-import.ts",
    "kg:validate": "npx tsx cli/kg-validate.ts all",
    "kg:benchmark": "npx tsx cli/kg-benchmark.ts all",
    "kg:iterate": "npx tsx cli/kg-iterate.ts",
    "validate": "npm run type-check && npm run lint && npm run kg:validate",
    "validate:schema": "npx tsx cli/kg-validate.ts schema",
    "validate:data": "npx tsx cli/kg-validate.ts data",
    "benchmark": "npx tsx cli/kg-benchmark.ts all",
    "benchmark:queries": "npx tsx cli/kg-benchmark.ts queries",
    "benchmark:traversal": "npx tsx cli/kg-benchmark.ts traversal",
    "iterate": "npx tsx cli/kg-iterate.ts start",
    "iterate:status": "npx tsx cli/kg-iterate.ts status",
    "iterate:dry": "npx tsx cli/kg-iterate.ts start --dry-run",
    "clean": "rm -rf node_modules .next coverage dist",
    "preinstall": "echo 'Installing dependencies...'",
    "postinstall": "echo 'Run npm run codegen to generate types'"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.562.0",
    "next": "16.1.2",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-force-graph-2d": "^1.29.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3",
    "@playwright/test": "^1.49.0",
    "@storybook/addon-essentials": "^8",
    "@storybook/addon-interactions": "^8",
    "@storybook/addon-onboarding": "^8",
    "@storybook/blocks": "^8",
    "@storybook/nextjs": "^8",
    "@storybook/react": "^8",
    "@storybook/test": "^8",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.2",
    "jsdom": "^25.0.1",
    "storybook": "^8",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
