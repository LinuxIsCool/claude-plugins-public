{
  "extends": "@docusaurus/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noEmit": true
  },
  "include": [
    "src/**/*",
    "docs/**/*",
    "iterations/**/*"
  ],
  "exclude": [
    "node_modules",
    "build",
    ".docusaurus"
  ]
}
